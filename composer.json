{
    "name": "endroid/twitter-bundle",
    "description": "Endroid Twitter Bundle for Symfony",
    "keywords": ["endroid", "twitter", "symfony", "bundle"],
    "homepage": "https://github.com/endroid/EndroidTwitterBundle",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jeroen van den Enden",
            "email": "info@endroid.nl",
            "homepage": "http://endroid.nl/"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "endroid/twitter": "~1.0"
    },
    "autoload": {
        "psr-4": {
            "Endroid\\Bundle\\TwitterBundle\\": [ "src/", "tests/" ]
        }
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    }
}
